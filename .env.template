# Environment Variables for FastMCP Cloud Deployment
# Copy this to .env and fill in your actual values

# Stripe Configuration
STRIPE_API_KEY=sk_test_your_stripe_secret_key_here
# Alternative name (for compatibility)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here

# Ollama Cloud Configuration (for kimi-k2.5:cloud)
# Obtain from: https://ollama.com/settings/keys (after `ollama signin`)
OLLAMA_API_KEY=your_ollama_api_key_here

# Optional: FastMCP Cloud specific settings
FASTMCP_ENVIRONMENT=production
FASTMCP_LOG_LEVEL=info
